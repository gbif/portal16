<div class="text-center" ng-if="eoi.notifications.components.REGISTRY !== 'OPERATIONAL'">
    <span class="error-block" translate="eoi.outage"></span>
</div>
<div layout="row" layout-align="end" flex ng-if="eoi.notifications.components.REGISTRY === 'OPERATIONAL'">
    <md-button type="submit" class="md-raised md-primary" ng-click="eoi.submitAttempted = true"><span translate="eoi.registerOrg">Register organization</span></md-button>
</div>